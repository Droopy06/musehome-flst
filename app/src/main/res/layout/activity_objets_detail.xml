<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_objets_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.flst.fges.musehome.ui.activity.ObjetsDetailActivity">

    <ImageView
        android:id="@+id/object_collection_imageview"
        android:maxHeight="300dp"
        android:minHeight="300dp"
        android:adjustViewBounds="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_nom_commum_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_nom_commum_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_groupe_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_groupe_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_genre_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_genre_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_espece_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_espece_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_auteur_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_auteur_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_annee_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_annee_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_pays_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_pays_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_ville_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_ville_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_lieu_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_lieu_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_nom_collection_textview"
            android:layout_width="0dp"
            android:layout_weight="0.5"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_nom_collection_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_collectionneur_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_collectionneur_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/header_localisation_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.flst.fges.musehome.ui.helper.CustomFontNeutrafaceTextView
            android:id="@+id/text_localisation_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</LinearLayout>
